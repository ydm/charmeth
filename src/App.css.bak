.charm-jumbotron .container {
    filter: blur(0px);
    -webkit-filter: blur(0px);
}

.charm-jumbotron-take {
    border: 0.25rem dashed rgba(157, 92, 139, 0.75);
    border-radius: 1rem;
}

.charm-underline-wavy {
    text-decoration: underline wavy #4d6788;
}

.charm-header-border {
    border-bottom: 1px solid;
    border-image: linear-gradient(to right, red, orange, yellow, green, blue, indigo, violet) 1;
